"source":"Components/Form.jsx","original":{"line":23,"column":4}},{"generated":{"line":74,"column":0},"source":"Components/Form.jsx","original":{"line":36,"column":1}},{"generated":{"line":74,"column":1},"source":"Components/Form.jsx","original":{"line":9,"column":0}},{"name":"Form","generated":{"line":76,"column":15},"source":"Components/Form.jsx","original":{"line":38,"column":15}}],"sources":{"Components/Form.jsx":"import React, { useState, useEffect } from 'react'\n\nconst initialState = {\n    task: \"\",\n    completed: false,\n    id: new Date().valueOf()\n}\n\nconst Form = () => {\n    const [form, setForm] = useState(initialState)\n    const [todo] = useState(JSON.parse(localStorage.getItem('todo')) || [])\n\n    useEffect(() => {\n        document.querySelector('input[type=\"text\"]').focus()\n    }, [])\n\n    const handleSubmit = (e) => {\n        setForm(JSON.parse(localStorage.getItem('todo')) || [])\n        todo.push(form)\n        localStorage.setItem('todo', JSON.stringify(todo))\n    }\n\n    return (\n        <div className=\"form-group\">\n            <form onSubmit={(e) => handleSubmit(e)} autoComplete=\"off\" >\n                <input\n                    type=\"text\"\n                    name=\"task\"\n                    value={form.task}\n                    onChange={e =>\n                        setForm({ ...form, [e.target.name]: e.target.value })} placeholder=\"Create a new todo...\"\n                />\n            </form>\n        </div>\n    )\n}\n\nexport default Form\n"},"lineCount":null}},"error":null,"hash":"1ee97e0e73250bc51b513b8a43b32d43","cacheData":{"env":{}}}